  "dependencies": {
    "concurrently": "^8.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "mongoose": "^7.2.2",
    "nodemon": "^2.0.22",
    "npm": "^9.7.1"
  }
}
